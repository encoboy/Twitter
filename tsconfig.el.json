{
  "compilerOptions": {
    "target": "esNext",
    "module": "commonjs",
    "outDir": "electrondist",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {},
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./electronsrc/**/*"
  ]
}